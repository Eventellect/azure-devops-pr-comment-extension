{
  "manifestVersion": 1,
  "id": "pr-comment-extension",
  "publisher": "TommiLaukkanen",
  "version": "1.0.19",
  "name": "PR Comment Task",
  "description": "Azure DevOps extension to easily add pull request comments from pipeline task",
  "public": false,
  "categories": [
    "Azure Repos",
    "Azure Pipelines"
  ],
  "tags": [
    "build",
    "pr",
    "comment",
    "pullrequest"
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/tlaukkanen/azure-devops-pr-comment-extension"
  },
  "icons": {
    "default": "images/dalle-128x128.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "contributions": [
    {
      "id": "pr-comment",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "buildandreleasetask"
      }
    }
  ],
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "LICENSE.txt"
    }
  },
  "files": [
    {
      "path": "buildandreleasetask"
    },
    {
      "path": "screenshots",
      "addressable": true
    }
  ],
  "links": {
    "repository": {
      "uri": "https://github.com/tlaukkanen/azure-devops-pr-comment-extension"
    }
  },
  "screenshots": [
    {
      "path": "screenshots/screen1.png"
    }
  ]
}