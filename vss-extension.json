{
  "manifestVersion": 1,
  "id": "pr-comment-extension",
  "publisher": "TommiLaukkanen",
  "version": "1.0.15",
  "name": "PR Comment Extension",
  "description": "Azure DevOps extension to easily add pull request comments from pipeline",
  "public": false,
  "categories": ["Azure Repos", "Azure Pipelines"],
  "repository": {
    "type": "git",
    "uri": "https://github.com/tlaukkanen/azure-devops-pr-comment-extension"
  },
  "icons": {
    "default": "images/dalle-128x128.png"
  },
  "targets": [
      {
          "id": "Microsoft.VisualStudio.Services"
      }
  ],
  "contributions": [
      {
          "id": "pr-comment",
          "type": "ms.vss-distributed-task.task",
          "targets": [
            "ms.vss-distributed-task.tasks"
        ],
        "properties": {
            "name": "buildandreleasetask"
        }
      }
  ],
  "content": {
    "details": {
      "path": "overview.md"
    }
  },
  "files": [
      {
          "path": "buildandreleasetask"
      }
  ]
}